{
  "name": "module-bundling-comparison-2016",
  "version": "0.0.1",
  "description": "module bundling test",
  "main": "module-bundling-comparison-2016.js",
  "scripts": {
    "browserify": "mkdirp dist && browserify src/index.js -o dist/bundle-browserify.js -t [ babelify --presets [ es2015 ] ]",
    "browserify-rollup": "mkdirp dist && browserify src/index.js -o dist/bundle-browserify-rollup.js -t [ rollupify --config rollup.config.js ]",
    "jspm": "mkdirp dist && jspm bundle src/index.js dist/bundle-jspm.js --skip-source-maps",
    "rollup": "mkdirp dist && rollup -c rollup.config.js",
    "webpack": "mkdirp dist && webpack"
  },
  "author": "Van Nguyen",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "jspm": "^0.17.0-beta.29",
    "mkdirp": "^0.5.1",
    "rollup": "^0.36.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollupify": "^0.3.4",
    "webpack": "^2.1.0-beta.25"
  },
  "jspm": {
    "configFile": "jspm.config.js",
    "dependencies": {
      "lodash": "npm:lodash@^4.16.3"
    },
    "devDependencies": {
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.16",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha"
    },
    "buildConfig": {
      "transpileES6": true,
      "minify": false
    },
    "overrides": {
      "npm:babel-runtime@5.8.38": {
        "main": false,
        "dependencies": {},
        "optionalDependencies": {
          "core-js": "^1.2.0"
        }
      },
      "npm:lodash@4.16.3": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      }
    }
  }
}
